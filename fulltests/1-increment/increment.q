function increment:long n:long
	return + n 1

function multiply:long a:long ; b:long
	return * a b

function divide:long a:long ; b:long
	return / a b

function subtract:long a:long ; b:long
	return - a b

function mod:long a:long ; b:long
	return % a b

function lessb:bool a:long ; b:long
	return < a b

function greaterb:bool a:long ; b:long
	return > a b

function lesseq:bool a:long ; b:long
	return <= a b

function greatereq:bool a:long ; b:long
	return >= a b

function equal:bool a:long ; b:long
	return = a b

function notequal:bool a:long ; b:long
	return <> a b

end